{
  "name": "mishusoft/website",
  "description": "Official website of Mishusoft",
  "homepage": "https://www.mishusoft.com/",
  "license": "proprietary",
  "authors": [
    {
      "name": "Mishusoft",
      "email": "support@mishusoft.com"
    }
  ],
  "support": {
    "email": "support@mishusoft.com",
    "issues": "https://www.mishusoft.com/framework-sample/issues",
    "source": "https://www.mishusoft.com/framework-sample"
  },
  "config": {
    "sort-order": true
  },
  "autoload": {
    "psr-4": {
      "Mishusoft\\": "Mishusoft/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mishusoft\\": "Sources/Mishusoft"
    }
  },
  "scripts": {
    "htaccess-update": "php ./Sources/Release.php -c  -s ./Sources/Mishusoft/http.htaccess -d ./.htaccess",
    "robots-update": "php ./Sources/Release.php -c  -s ./Sources/Mishusoft/robots.txt -d ./robots.txt",
    "index-php-update": "php ./Sources/Release.php -c  -s ./Sources/Mishusoft/app.php -d ./index.php",
    "index-html-update": "php ./Sources/Release.php -c  -s ./Sources/Mishusoft/app.html -d ./index.html",
    "framework-update": "php ./Sources/Release.php -c  -s ./Sources/Mishusoft/Framework -d ./Mishusoft/Framework",
    "ema-update": "php ./Sources/Release.php -c  -s ./Sources/Mishusoft/Ema -d ./Mishusoft/Ema",
    "packages-update": "php ./Sources/Release.php -c  -s ./Sources/Mishusoft/Packages -d ./Packages",
    "themes-update": "php ./Sources/Release.php -t  -s ./Sources/Mishusoft/Views/Themes -d ./Mishusoft/Themes",
    "widgets-update": "php ./Sources/Release.php -c  -s ./Sources/Mishusoft/Widgets -d ./Mishusoft/Widgets",
    "widgets-components-update": "php ./Sources/Release.php -w  -s ./Sources/Mishusoft/Views/Widgets -d ./Mishusoft/Widgets/Views",
    "packages-components-update": "php ./Sources/Release.php -c  -s ./Sources/Mishusoft/Views/Templates/Packages -d ./Packages",
    "copy-media": "mishusoft-release -copy  -s ./Sources/Assets/media -d ./Storages/0/media",
    "copy-errors-page": "mishusoft-release -copy  -s ./Sources/Mishusoft/V -d ./Storages/0/media",
    "phpcs": "phpcs",
    "phpcbf": "phpcbf"
  },
  "require": {
    "php": "8.*",
    "verot/class.upload.php": "^2.1",
    "stripe/stripe-php": "^7.77",
    "phpmailer/phpmailer": "^6.4",
    "smarty/smarty": "^3.1",
    "geoip2/geoip2": "^2.11",
    "ext-json": "*",
    "ext-zip": "*",
    "ext-openssl": "*",
    "ext-dom": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6",
    "friendsofphp/php-cs-fixer": "^3.0"
  }
}
