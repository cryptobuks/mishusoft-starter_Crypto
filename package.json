{
  "name": "lastest.mishusoft.com",
  "version": "2.0.35",
  "description": "Mishusoft official app development",
  "scripts": {
    "composer:install": "php ./requirements/composer-install.php",
    "composer:update": "php ./requirements/composer.phar install",
    "build": "webpack",
    "build:prod": "webpack --env target=production",
    "watch": "webpack -w && npm run update:rv",
    "start:server.dev": "php -S localhost:8080",
    "start:server.wp": "npx webpack serve",
    "update:rv": "php ./sources/mishusoft-company/Release.php -u",
    "lighthouse": "CHROME_PATH=/usr/bin/vivaldi-stable lighthouse --quiet --no-update-notifier --no-enable-error-reporting --output=json --chrome-flags=\"--headless\" http://localhost",
    "src:backup": "node ./sources/Utils/backup-src",
    "src:zip": "node ./sources/Utils/zip-src",
    "remove:webfonts": "rm -rvf ./storages/app/webfonts",
    "packages:outdated": "npm outdated",
    "packages:upgrade": "npx npm-check-updates -u --timeout 60000 && npm install"
  },
  "keywords": [],
  "author": "Al-Amin Ahamed (Abir)",
  "license": "Property",
