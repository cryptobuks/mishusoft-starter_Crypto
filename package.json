{
  "name": "lastest.mishusoft.com",
  "version": "2.0.35",
  "description": "Mishusoft official app development",
  "scripts": {
    "composer:install": "php ./requirements/composer-install.php",
    "composer:update": "php ./requirements/composer.phar install",
    "build": "webpack",
    "build:prod": "webpack --env target=production",
    "watch": "webpack -w && npm run update:rv",
    "start": "php -S localhost:8080 router.php",
    "start:server.dev": "php -S localhost:8080 router.php",
    "start:php80.win": "tools\\php80\\php.exe -S localhost:8080 router.php",
    "start:php74.win": "tools\\php74\\php.exe -S localhost:8080 router.php",
    "start:php74.linux": "php7 -S localhost:8080 router.php",
    "start:server.wp": "npx webpack serve",
    "update:rv": "php ./sources/mishusoft-company/Release.php -u",
    "lighthouse": "CHROME_PATH=/usr/bin/vivaldi-stable lighthouse --quiet --no-update-notifier --no-enable-error-reporting --output=json --chrome-flags=\"--headless\" http://localhost",
    "src:backup": "node ./sources/Utils/backup-src",
    "src:zip": "node ./sources/Utils/zip-src",
    "remove:webfonts": "rm -rvf ./storages/app/webfonts",
    "remove:framework": "rm -rvf ./Framework",
    "packages:outdated": "npm outdated",
    "packages:upgrade": "npx npm-check-updates -u --timeout 100000 && npm install"
  },
  "keywords": [],
  "author": "Al-Amin Ahamed (Abir)",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@types/sass": "^1.16.1",
    "archiver": "^5.3.0",
    "autoprefixer": "^10.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "critical": "^4.0.1",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "cssnano": "^5.0.8",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.0",
    "mini-css-extract-plugin": "^2.4.2",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.43.2",
    "sass-loader": "^12.2.0",
    "semver-parser": "^4.0.0",
    "stripe": "^8.182.0",
    "style-loader": "^3.3.0",
    "sugarss": "^4.0.1",
    "svgo": "^2.7.0",
    "sweetalert": "^2.1.2",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-font-preload-plugin": "^1.3.0",
    "webpack-nano": "^1.1.1",
    "webpack-preprocessor": "^0.1.12",
    "webpack-preprocessor-loader": "^1.1.4",
    "webpack-remove-empty-scripts": "^0.7.1",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "imagemin": "^8.0.1",
    "lodash": "^4.17.21",
    "tailwindcss": "^2.2.17"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mralaminahamed/lastest.mishusoft.com.git"
  }
}
