{
  "name": "development",
  "version": "2.0.16",
  "description": "Mishusoft official app development",
  "scripts": {
    "build": "webpack && npm run backup-src",
    "build:prod": "webpack --env.target=production && npm run update-rv && npm run remove-webfonts && npm run backup-src",
    "watch": "webpack -w && npm run release && npm run update-rv",
    "php-devServer": "php -S localhost:8080",
    "update-rv": "php ./Sources/Release.php -u",
    "lighthouse": "CHROME_PATH=/usr/bin/vivaldi-stable lighthouse --quiet --no-update-notifier --no-enable-error-reporting --output=json --chrome-flags=\"--headless\" http://localhost",
    "backup-src": "node ./Sources/Utils/backup-src",
    "zip-src": "node ./Sources/Utils/zip-src",
    "remove-webfonts": "rm -rvf ./Storages/0/webfonts"
  },
  "keywords": [],
  "author": "Mr Al Amin Ahamed Abir",
  "license": "ISC",
  "devDependencies": {
    "@types/sass": "^1.16.0",
    "archiver": "^4.0.2",
    "autoprefixer": "^9.8.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "critical": "^3.0.0",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "file-loader": "^6.2.0",
    "formidable": "^1.2.2",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "javascript-obfuscator": "^2.12.0",
    "mini-css-extract-plugin": "^0.9.0",
    "normalize-scss": "^7.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.15",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.32.12",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "semver-parser": "^3.1.0",
    "stripe": "^8.127.0",
    "style-loader": "^1.3.0",
    "sugarss": "^2.0.0",
    "sweetalert": "^2.1.2",
    "terser-webpack-plugin": "^3.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
