{
  "name": "mishusoft-starter",
  "version": "3.01.00",
  "description": "Mishusoft official app development",
  "keywords": [
    "mishusoft",
    "framework"
  ],
  "homepage": "https://www.mishusoft.com/",
  "bugs": {
    "url": "https://gitlab.com/mralaminahamed/mishusoft-starter/issues",
    "email": "alamin@mishusoft.com"
  },
  "license": "GPL-3.0-only",
  "author": {
    "name": "Al Amin Ahamed",
    "email": "alamin@mishusoft.com",
    "url": "https://gitlab.com/mralaminahamed/"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/al-amin-ahamed"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/mralaminahamed/mishusoft-starter.git"
  },
  "scripts": {
    "pre-install": "npm install -g yarn webpack lighthouse gifsicle && yarn set version berry",
    "composer:install": "php ./requirements/composer-install.php",
    "composer:update": "php ./requirements/composer.phar install",
    "clean": "npm prune",
    "build:app": "webpack --env product=app",
    "build:framework": "webpack --env product=framework",
    "build:webfonts:app": "webpack --env product=webfonts target=app",
    "build:webfonts:framework": "webpack --env product=webfonts target=framework",
    "build:webfonts": "npm run build:webfonts:app && npm run build:webfonts:framework",
    "build": "npm run build:app && npm run build:framework && npm run build:webfonts",
    "build:prod:app": "webpack --env product=app mode=production",
    "build:prod:framework": "webpack --env product=framework mode=production",
    "build:prod:webfonts:app": "webpack --env product=webfonts target=app mode=production",
    "build:prod:webfonts:framework": "webpack --env product=webfonts target=framework mode=production",
    "build:prod:webfonts": "npm run build:prod:webfonts:app && npm run build:prod:webfonts:framework",
    "build:prod": "npm run build:prod:app && npm run build:prod:framework && npm run build:prod:webfonts",
    "debug:app": "node --trace-deprecation node_modules/webpack/bin/webpack.js --env product=app",
    "debug:framework": "node --trace-deprecation node_modules/webpack/bin/webpack.js --env product=framework",
    "debug": "npm run debug:app && npm run debug:framework",
    "watch:app": "webpack -w --env product=app",
    "watch:framework": "webpack -w --env product=framework",
    "watch": "npm run watch:app && npm run watch:framework",
    "start": "php -S localhost:8000 public_html/index.php",
    "start:lsws": "sudo /usr/local/lsws/bin/lswsctrl start",
    "start:base": "php -S localhost:8000 router.php",
    "start:server.dev": "php -S localhost:8000 public_html/index.php",
    "start:php81.win": "tools\\php81\\php.exe -S localhost:8000 public_html/index.php",
    "start:php80.win": "tools\\php80\\php.exe -S localhost:8000 public_html/index.php",
    "start:php74.win": "tools\\php74\\php.exe -S localhost:8000 public_html/index.php",
    "start:php74.linux": "php7 -S localhost:8000 public_html/index.php",
    "start:server.wp": "npx webpack serve",
    "update:rv": "php ./sources/mishusoft-company/Release.php -u",
    "lighthouse": "lighthouse --quiet --no-update-notifier --no-enable-error-reporting --output=json --chrome-flags=\"--headless\" http://localhost:8000 --budget-path=./public_html/budget.json --output-path=./tmp/lighthouse/report.json",
    "lighthouse:normal": "lighthouse --quiet --no-update-notifier --no-enable-error-reporting --chrome-flags=\"--headless\" http://localhost:8000 --budget-path=./public_html/budget.json --output-path=./tmp/lighthouse/report.html",
    "lighthouse:basic": "lighthouse --chrome-flags=\"--headless\" http://localhost:8000 --budget-path=./public_html/budget.json --output-path=./tmp/lighthouse/report.html",
    "lighthouse:web": "lighthouse --chrome-flags=\"--headless\" http://localhost:8000 --budget-path=./public_html/budget.json --output-path=./tmp/lighthouse/report.html --web",
    "src:backup": "node ./Utils/backup-src",
    "src:zip": "node ./Utils/zip-src",
    "remove:framework": "rm -rvf ./Framework",
    "packages:outdated": "npm outdated",
    "packages:upgrade": "npx npm-check-updates -u && npm install && composer upgrade",
    "//packages:upgrade": "npx npm-check-updates -u --timeout 10000000 && npm install && composer upgrade"
  },
  "devDependencies": {
    "@types/sass": "^1.43.1",
    "archiver": "^5.3.1",
    "autoprefixer": "^10.4.7",
    "chrome-launcher": "^0.15.0",
    "clean-webpack-plugin": "^4.0.0",
    "constants": "^0.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "critical": "^4.0.1",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cssnano": "^5.1.9",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "lighthouse": "^9.6.1",
    "lodash": "^4.17.21",
    "merge2": "^1.4.1",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "npm-check": "^5.9.2",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.6.0",
    "prettier": "^2.6.2",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "semver-parser": "^4.0.1",
    "stripe": "^9.2.0",
    "style-loader": "^3.3.1",
    "sugarss": "^4.0.1",
    "svgo": "^2.8.0",
    "sweetalert": "^2.1.2",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-font-preload-plugin": "^1.5.0",
    "webpack-nano": "^1.1.1",
    "webpack-preprocessor": "^0.1.12",
    "webpack-preprocessor-loader": "^1.1.4",
    "webpack-remove-empty-scripts": "^0.8.0",
    "webpack-stream": "^7.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 7.0.20"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 8",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  }
}
